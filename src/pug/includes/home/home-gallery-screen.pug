-
  var slides = [
    {title: "ІГРОВА ЗОНА"},
    {title: "ВБИРАЛЬНЯ"},
    {title: "СПАЛЬНЯ"},
    {title: "ЗОВНІШНІЙ ДВІР"},
    {title: "БАСЕЙН"},
    {title: "ДИТЯЧИЙ МАЙДАНЧИК"},
    {title: "ВНУТРІШНІЙ ДВІР"},
    {title: "СПОРТИВНИЙ МАЙДАНЧИК"},
  ]

div(class="home-gallery-screen "+galleryBlockCustomClass)#gallery
  .home-gallery-screen__title.text-style-1920-h-2.text-style-768-h-2.color-white-bb Галерея
  .frame__nav
    each slide in slides
      button.frame__nav-button.unbutton.text-style-1920-button #{slide.title}
  <div class="slides">
    each slide in slides
      div.slide
        div.slide__inner
          div.slide__img
            div.slide__img-inner(style="background-image:url(./assets/images/home/gallery/1.jpg)")
            div.slide__img-inner2(style="background-image:url(./assets/images/home/gallery/1.jpg)")
  </div>
  .home-gallery-screen__nav
    .slider-nav-button.text-style-1920-button(data-gallery="prev")
      <svg width="25" height="24" viewBox="0 0 25 24" fill="none" xmlns="http://www.w3.org/2000/svg">
        <path d="M11.8846 20L4.5 12M4.5 12L11.8846 4M4.5 12L20.5 12" stroke="#0A0F13"/>
      </svg>
    .home-gallery-screen__nav-digits
      span.color-white-bb(data-gallery-current) 01
      span.color-white-bb /
      span.color-white-bb(data-gallery-all) #{slides.length < 10 ? "0" + slides.length : slides.length}
    .slider-nav-button(data-gallery="next")
      <svg width="25" height="24" viewBox="0 0 25 24" fill="none" xmlns="http://www.w3.org/2000/svg">
        <path d="M13.1154 20L20.5 12M20.5 12L13.1154 4M20.5 12L4.5 12" stroke="#0A0F13"/>
      </svg>
